{
  "module": {
    "name": "entry",
    "type": "entry",
    "description": "$string:module_desc",
    "mainElement": "HomeAbility",
    "deviceTypes": [
      "phone",
      "tablet",
      "2in1",
      "wearable"
    ],
//    "requestPermissions": [
//      {
//        "name": "ohos.permission.READ_USER_STORAGE",
//        "reason": "用于读取数据库文件"
//      },
//      {
//        "name": "ohos.permission.WRITE_USER_STORAGE",
//        "reason": "用于写入数据库文件"
//      }
//    ]
    "deliveryWithInstall": true,
    "installationFree": false,
    "pages": "$profile:main_pages",
    "abilities": [
      {
        "name": "HomeAbility",
        "srcEntry": "./ets/homeability/HomeAbility.ets",
        "description": "$string:HomeAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:HomeAbility_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background",
        "exported": true,
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ]
      },
      {
        "name": "EntryAbility",
        "srcEntry": "./ets/entryability/EntryAbility.ets",
        "description": "$string:EntryAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:EntryAbility_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background",
      },
      {
        "name": "LevelAbility",
        "srcEntry": "./ets/Levelability/LevelAbility.ets",
        "description": "$string:Levelability_desc",
        "icon": "$media:layered_image",
        "label": "$string:Levelability_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background"
      },
      {
        "name": "LoginAbility",
        "srcEntry": "./ets/loginability/LoginAbility.ets",
        "description": "$string:LoginAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:LoginAbility_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background"
      },
      {
        "name": "RegisterAbility",
        "srcEntry": "./ets/registerability/RegisterAbility.ets",
        "description": "$string:RegisterAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:RegisterAbility_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background"
      }
    ],
    "extensionAbilities": [
      {
        "name": "EntryBackupAbility",
        "srcEntry": "./ets/entrybackupability/EntryBackupAbility.ets",
        "type": "backup",
        "exported": false,
        "metadata": [
          {
            "name": "ohos.extension.backup",
            "resource": "$profile:backup_config"
          }
        ],
      },
    ]
  }
}